{
  "name": "@khokhlachev/vercel",
  "version": "1.0.2",
  "description": "Collection of helpers specific for Vercel PaaS",
  "author": "Sergey Khokhlachev <sergey@khokhlachev.com>",
  "homepage": "https://github.com/khokhlachev/helper-packages#readme",
  "license": "MIT",
  "main": "index.js",
  "module": "esm/index.js",
  "types": "index.d.ts",
  "typings": "index.d.ts",
  "sideEffects": false,
  "directories": {
    "esm": "esm",
    "test": "__tests__"
  },
  "files": [
    "esm",
    "*.md",
    "index.js",
    "index.d.ts",
    "connect-middleware.js",
    "connect-middleware.d.ts",
    "push-env.js",
    "push-env.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/khokhlachev/helper-packages.git"
  },
  "scripts": {
    "clear": "rm -rf {esm,lib}",
    "build:cjs": "tsc",
    "build:es": "tsc -m esNext --outDir esm",
    "build": "yarn clear && yarn build:cjs && yarn build:es",
    "prepublish": "yarn build",
    "postpublish": "yarn clear",
    "test": "echo \"Error: run tests from root\" && exit 1"
  },
  "bugs": {
    "url": "https://github.com/khokhlachev/helper-packages/issues"
  },
  "dependencies": {
    "@khokhlachev/utils": "^1.0.4",
    "arg": "^5.0.1"
  },
  "devDependencies": {
    "@types/node": "^16.7.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
