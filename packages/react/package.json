{
  "name": "@khokhlachev/react",
  "version": "1.0.9",
  "description": "Collection of helpers for React.js",
  "author": "Sergey Khokhlachev <sergey@khokhlachev.com>",
  "homepage": "https://github.com/khokhlachev/helper-packages#readme",
  "license": "MIT",
  "main": "lib/index.js",
  "module": "esm/index.js",
  "types": "lib/index.d.ts",
  "typings": "lib/index.d.ts",
  "sideEffects": false,
  "directories": {
    "esm": "esm",
    "test": "__tests__"
  },
  "files": [
    "esm",
    "lib",
    "*.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/khokhlachev/helper-packages.git"
  },
  "scripts": {
    "clear": "rm -rf {esm,lib}",
    "build:cjs": "tsc",
    "build:es": "tsc -m esNext --outDir esm",
    "build": "yarn clear && yarn build:cjs && yarn build:es",
    "prepublish": "yarn build",
    "test": "echo \"Error: run tests from root\" && exit 1"
  },
  "bugs": {
    "url": "https://github.com/khokhlachev/helper-packages/issues"
  },
  "dependencies": {
    "@khokhlachev/utils": "^1.0.8",
    "react-use": "^17.2.0"
  },
  "devDependencies": {
    "@types/body-scroll-lock": "3.1.0",
    "@types/react": "17.0.19",
    "body-scroll-lock": "^3.1.5",
    "react": "17.0.2"
  },
  "peerDependencies": {
    "body-scroll-lock": "^3.1.5",
    "react": ">=16.0.0 || >=17.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
