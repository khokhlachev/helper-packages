{
  "name": "@khokhlachev/utils",
  "version": "1.0.3",
  "description": "Personal collection of common utilies",
  "author": "@khokhlachev",
  "license": "MIT",
  "homepage": "https://github.com/khokhlachev/helper-packages",
  "main": "lib/index.js",
  "module": "esm/index.js",
  "types": "lib/index.d.ts",
  "typings": "lib/index.d.ts",
  "sideEffects": false,
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "esm",
    "*.md",
    "index.js",
    "index.d.ts",
    "logger.js",
    "logger.d.ts",
    "math.js",
    "math.d.ts",
    "types.js",
    "types.d.ts"
  ],
  "scripts": {
    "clear": "rm -rf {lib,esm}",
    "test": "echo \"Error: run tests from root\" && exit 1",
    "prepublish": "yarn clear && yarn build",
    "postpublish": "yarn clear",
    "build:cjs": "tsc",
    "build:es": "tsc -m esNext --outDir esm",
    "build": "yarn build:cjs && yarn build:es"
  },
  "dependencies": {
    "chalk": "^4.1.2"
  },
  "devDependencies": {
    "@types/node": "^16.7.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
